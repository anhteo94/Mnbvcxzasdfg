local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/PhamNghiaIos/Fluent-UI/main/ui"))()
local Window = Library:CreateWindow({
    Title = "Ôº∞Ôº®·∫†Ôº≠ ÔºÆÔºßÔº®ƒ®Ôº° üçè Ôº©ÔºØÔº≥",
